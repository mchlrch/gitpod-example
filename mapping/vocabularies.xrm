language-tags {
	en
}

vocabulary rdf {
	prefix "rdf" "http://www.w3.org/1999/02/22-rdf-syntax-ns#"

	properties
		^type
}

vocabulary xsd {
	prefix "xsd" "http://www.w3.org/2001/XMLSchema#"

	datatypes
		string
}

vocabulary ex {
	prefix "ex" "http://example.com/ns#"

	classes
		Employee
		Department

	properties
		name
		department
}

vocabulary schema {
	prefix "schema" "http://schema.org/"

	classes
		Place

	properties
		location
		name
}

